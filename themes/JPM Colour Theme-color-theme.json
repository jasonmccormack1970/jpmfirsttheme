	"tokenColors": [
		{
			"settings": {
				"background": "#0b0e14",
				"foreground": "#bfbdb6"
			}
		},
		{
			"name": "Comment",
			"scope": [
				"comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#acb6bf8c"
			}
		},
		{
			"name": "String",
			"scope": [
				"string",
				"constant.other.symbol"
			],
			"settings": {
				"foreground": "#aad94c"
			}
		},
		{
			"name": "Regular Expressions and Escape Characters",
			"scope": [
				"string.regexp",
				"constant.character",
				"constant.other"
			],
			"settings": {
				"foreground": "#95e6cb"
			}
		},
		{
			"name": "Number",
			"scope": [
				"constant.numeric"
			],
			"settings": {
				"foreground": "#d2a6ff"
